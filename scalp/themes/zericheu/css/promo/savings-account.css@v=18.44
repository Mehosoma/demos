}

.footer__items {
    padding: 40px 40px 40px 0;
}

.footer__items a {
    display: flex;
    flex-direction: column;
}

.footer__items a {
    font-size: 16px;
    line-height: 1.63;
    text-align: left;
    color: #fff;
    text-decoration: underline;
}

.footer__items a:hover {
    color: #fff;
}

button, input, select, textarea {
    padding: 7px;
    border-radius: 100px;
    border: 1px solid #fff;
}

.description-container {
    min-height: 150px;
    background-color: rgba(93, 93, 93, 0.39);
}

.description {
    margin: 0 auto;
    max-width: 67.6em;
}

article {
    padding: 50px 0;
    text-align: left;
    font-size: 12px;
    line-height: 1.33;
    color: #fff;
}

@media only screen and (min-width: 64em) {
    .col-md-3 {
        max-width: 23% !important;
    }
}

