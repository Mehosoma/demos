.search-icon {
    background-image: url('data:image/svg+xml;charset=utf-8;base64,');
    cursor:pointer;
    height:31px;
    width:31px;
    background-color: transparent;
}
#searchButton.blue {
    /*filter: invert(100%)*/
    filter: grayscale(100%) brightness(26%) sepia(100%) hue-rotate(-180deg) saturate(700%) contrast(0.8);
}
#search-popup-logo {
    background-image: url("data:image/svg+xml;charset=utf-8;base64,");
    width: 118px;
    display: block;
    background-repeat: no-repeat;
    flex-shrink: 0;
    height: 61px;
    background-position: center;
}

#search-term {
    width: calc(100% - 35px);
    background-color: transparent;
    border: none;
    font-size: 24px;
    color: #fff;
    outline: 0;
    height: 61px;
}

#search-black-popup {
    position: fixed;
    width: 100vw;
    height: 100vh;
    background: rgba(28,28,28,.98);
    top: 0;
    left: 0;
    z-index: 100;

    display: flex;
    align-items: flex-start;
    justify-content: center;

    transition: opacity .5s ease-in-out;
    opacity: 1;
}

#search-black-popup.hidden {
    transition: opacity .5s ease-in-out;
    opacity: 0;
    display: none;
}

.search-container{
    display: flex;
    align-items: flex-start;
    justify-content: center;
    width: 100%;
    max-width: 1220px;

    margin: 10vh 20px 0;
}

#close-popup-button {
    cursor: pointer;
    width: 30px;
    height: 61px;
    position: relative;
    text-shadow: 0 1px 0 #fff;
    opacity: .6;
    flex-shrink: 0;
}

#close-popup-button:before, #close-popup-button:after {
    width: 40px;
    height: 2px;
    border-radius: 2px;
    position: absolute;
    top: 0;
    bottom: 0;
    left: -5px;
    right: auto;
    margin: auto;
    display: block;
    background-color: #FFF;
}
#close-popup-button:before {
    content: '';
    transform: rotate(45deg);
}
#close-popup-button:after {
    content: '';
    transform: rotate(-45deg);
}

.search-col {
    width: 100%;
    margin: 0 40px;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.search-row {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid white;
}

.search-box{
    padding: 5px 5px 15px;
    height: 100%;
    width: 100%;
}

.search-box a {
    color: #fff;
    display: block;
    padding: 4px 12px;
    text-decoration: none;
    font-size: 18px;
}

@media (max-width: 766px) {
    #search-popup-logo {
        display: none;
    }
}