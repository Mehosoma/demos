.select-nav a {
    font-size: 14px;
}

.page-trading header.header {
    min-height: 350px;
}


.page-trading .video-wrapper {
    background: transparent url(/themes/zericheu/images/promo/tariffs/businessman_bg.png) no-repeat top center;
}


.banner-preview {
    width: 100%;
    height : 100%;
    cursor: pointer;
}

.page-trading .header-content .header-content-title {
    margin-top: 50px;
    max-width: 700px;
    margin-bottom: 0;
    font-size: 45px;
}

a:-webkit-any-link:hover {
    cursor: pointer;
    text-decoration: none;
    /*-webkit-transform: scale(1.1);*/
}

.background {
    background-color: #f6f9fb;
    min-height: 1050px;
}


.nav-main {
    border-bottom: 1px solid rgba(0,0,0,.16);
    width: 100%;
}

footer .nav-main a {
    text-transform: uppercase;
    color: #000;
    margin: 0;
    float: none;
    line-height: 23px;
    display: inline-block;
    padding: 10px 0;
    font-weight: 700;
    font-size: 13px;
}

.grey-text {
    line-height: 1.56;
    color: #999;
}
footer .socials {
    width: 100%;
}


.btn-full-width, a.btn-full-width {
    width: 100%;
    border: none!important;
    line-height: 52px;
    height: 75px;
    color: #fff;
    text-align: center;
    cursor: pointer;
    display: inline-block;
    -webkit-transition: .1s linear;
    transition: .1s linear;
    -webkit-transition-property: -webkit-transform,background-color,color;
    transition-property: transform,background-color,color;
    padding: 14px 35px;
    font-size: 15px;
    font-weight: 500;
    min-width: 162px;
    text-transform: uppercase;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    letter-spacing: .6px;
}

.no-gutters {
    padding-right: 0;
    padding-left: 0;
}

.news-item-main {
    padding: 0 0 0 0;
    -webkit-box-flex: 1;
    -webkit-flex: auto;
    -ms-flex: auto;
    flex: auto;
}

.big-padding {
    padding: 20px;
}

.height-100 {
