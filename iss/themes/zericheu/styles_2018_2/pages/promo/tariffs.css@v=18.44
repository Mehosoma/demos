.tip {
    position: relative;
    display: inline-block;
    border-bottom: 1px dotted black;
}

.tip .tiptext {
    bottom: 100%;
    /*right: -150px;*/
    visibility: hidden;
    width: 300px;
    background-color: white;
    color: black;
    border-radius: 6px;
    padding: 25px;
    box-shadow: 0 0 15px rgba(0, 0, 0, .38);
    position: absolute;
    z-index: 1;
    text-align: left;
    word-wrap: break-word;
}

.tip:hover .tiptext {
    visibility: visible;
}


.violet {
    color: #4753d1;
}


.drop-down {
    cursor : pointer;
}

.bold-desktop {
    font-weight: 300;
    font-size: 12px;
}

#additional {
    display: none;
}

.page-trading .header-content .breadcrumbs {
    margin-top: 10px;
    margin-bottom: 0;
    font-size: 16px;
}

.flink {
    padding-left: 45px;
    min-height: 40px;
    background-position: left top;
    background-repeat: no-repeat;
}
.flink.one-line {
    padding-top: 10px;
}
.flink.pdf {
    background-image: url(/themes/zericheu/images/pdf.png);
}
.flink.doc {
    background-image: url(/themes/zericheu/images/doc.png);
}
li.flink {
    margin-bottom: 10px;
}

