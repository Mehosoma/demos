    box-shadow: 7px 0 7px 0 hsla(0, 0%, 83%, .5);
    position: relative;
    z-index: 2
}

.shares tr:hover td:after {
    border-top-color: #fff
}

.shares tr:hover td:first-child {
    -webkit-box-shadow: 0 0 7px 0 hsla(0, 0%, 83%, .5);
    box-shadow: 0 0 7px 0 hsla(0, 0%, 83%, .5)
}

.shares tr:hover td:last-child {
    -webkit-box-shadow: 0 0 3px 0 hsla(0, 0%, 83%, .5);
    box-shadow: 0 0 3px 0 hsla(0, 0%, 83%, .5)
}

.shares tr:hover + tr > td:after {
