.main-articles {
    display: flex!important;
    flex-direction: row;
    position: relative;
    overflow: hidden;
    padding-bottom: 48px
}

.main-articles-inner .owl-stage-outer .owl-stage:after,
.main-articles-inner:after {
    display: block;
    clear: both
}

.main-articles-inner {
    max-width: 808px;
    margin: 0 auto;
    position: relative
}

.main-articles-inner .owl-item {
    float: left;
    max-width: 808px
}

.main-articles-inner .owl-dots {
    position: absolute;
    bottom: -48px;
    left: 0;
    right: 0;
    z-index: 2;
    text-align: center
}

.main-articles-inner .owl-dots .owl-dot {
    width: 12px;
    height: 12px;
    background-color: #748BFA;
    font-size: 0;
    display: inline-block;
    border-radius: 6px;
    margin-right: 12px;
    opacity: .22;
    cursor: pointer;
    margin-top: 36px;
    transition: .1s linear
}

.main-articles-inner .owl-dots .owl-dot:hover {
    opacity: .55
}

.main-articles-inner .owl-dots .owl-dot:last-child {
    margin-right: 0
}

.main-articles-inner .owl-dots .owl-dot.active {
    opacity: 1;
    cursor: default
}

.main-articles-inner .owl-nav {
    display: block!important;
    position: static;
    z-index: 50!important
}

.main-articles-inner .owl-nav .owl-next,
.main-articles-inner .owl-nav .owl-prev {
    top: 0;
    width: 40px;
    height: 100%;
    position: absolute;
    z-index: 1;
    transform-style: preserve-3d;
    transform: translateZ(2000px);
    border: none;
    opacity: .5;
    background-color: transparent
}

.main-articles-inner .owl-nav .owl-next:after,
.main-articles-inner .owl-nav .owl-prev:after {
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    opacity: 0;
    width: auto;
    height: auto;
    border: none;
    content: '';
    transition: .1s linear;
    background-image: linear-gradient(to left, rgba(22, 51, 105, .8) 0, transparent 100%)
}

.main-articles-inner .owl-nav .owl-next:hover,
.main-articles-inner .owl-nav .owl-prev:hover {
    opacity: .7!important
}

.main-articles-inner .owl-nav .owl-next:hover:after,
.main-articles-inner .owl-nav .owl-prev:hover:after {
    opacity: .5
}

.main-articles-inner .owl-nav .owl-next:active,
.main-articles-inner .owl-nav .owl-prev:active {
    opacity: .77!important
}

.main-articles-inner .owl-nav .owl-prev {
    right: auto;
    border-radius: 0 5px 5px 0;
    background-image: linear-gradient(to right, rgba(0, 0, 0, .8) 20%, transparent 100%)
}

@media all and (max-width:767px) {
    .main-articles-inner {
        padding: 0 30px
    }
    .main-articles-inner .owl-nav .owl-prev {
        background-image: linear-gradient(to right, rgba(0, 0, 0, .8) 50%, transparent 100%)
    }
}

.main-articles-inner .owl-nav .owl-next {
    background-image: linear-gradient(to left, rgba(0, 0, 0, .8) 20%, transparent 100%);
    border-radius: 5px 0 0 5px;
    right: 0
}

@media all and (max-width:767px) {
    .main-articles-inner .owl-nav .owl-next {
        background-image: linear-gradient(to left, rgba(0, 0, 0, .8) 50%, transparent 100%)
    }
}

.main-articles article {
    max-width: 808px;
    height: 444px;
    margin-right: 36px;
    background-position: center;
    background-size: cover;
    padding: 40px 33px;
    position: relative;
    overflow: hidden;
    border-radius: 7px
}

.main-articles article:last-child {
    margin-right: 0
}

.main-articles article:after {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: linear-gradient(0deg, transparent 0, #000 100%);
    z-index: 3;
    opacity: .78;
    transform-style: preserve-3d;
    transform: translateZ(0);
    border-radius: 5px
}

.main-articles article>* {
    position: relative
}

.main-articles article .video {
    top: 0;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1!important;
    transform-style: preserve-3d;
    transform: translateZ(0);
    overflow: hidden;
    background-color: transparent
}

.main-articles article .article-wrap-info {
    height: 100%;
