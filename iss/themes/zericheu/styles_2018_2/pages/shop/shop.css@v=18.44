}

.shares-item td {
    vertical-align: middle;
    position: relative;
    padding: 34px 10px
}

.shares-item td:after {
    border-top: 1px solid #e7e7e7;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    right: 0
}

.shares-item td:first-child {
    padding-left: 40px
}

.shares-item td:first-child:after {
    left: 40px
}

.shares-item td:nth-child(3) {
