}

.shares-item td:nth-child(4) {
    min-width: 130px
}

.shares-item td:last-child {
    text-align: right;
    padding-right: 40px
}

.shares-item td:last-child:before {
    display: block;
    position: absolute;
    background-color: #fff;
    left: -4px;
    top: 1px;
    bottom: 5px;
    width: 10px;
    z-index: 2
}

.shares-item td:last-child:after {
    right: 40px
