	width: 180px;
	padding-left: 80px;
	text-align: left;
	background-repeat: no-repeat;
	background-position: left center;
}
.fr {
	background-image: url(data:image/jpeg;base64,);
}
.sr {
	background-image: url(data:image/jpeg;base64,);
}
.vr {
	background-image: url(data:image/jpeg;base64,);
}

	.line1,
	.line1,
	.line1,
	.nominations .line1 {
		font-size: 37px;
		font-weight: bold;
	}

	.line2,
	.line2,
	.line2,
	.nominations .line2 {
		margin-bottom: 0;
		font-size: 20px;
	}

.nominations {
	margin-bottom: 30px;
}

	.nominations td {
		vertical-align: middle;
	}

		.nominations .h3 {
			border-bottom: 1px solid #7fac31;
		}

		.nominations .bold {
			font-size: 22px;
			font-weight: bold;
			line-height: 25px;
		}

.advantage {
	padding-left: 80px;
	background-repeat: no-repeat;
	background-position: left top;
}
.advantage.item1 {
	background-image: url(data:image/jpeg;base64,);
}
.advantage.item2 {
	background-image: url(data:image/jpeg;base64,);
}
.advantage.item3 {
	background-image: url(data:image/jpeg;base64,);
}
.advantage.item4 {
	background-image: url(data:image/jpeg;base64,);
}

.block-mechel {
	padding: 20px 0 0 120px;
	height: 290px;
	background: url(../../images/promo/investor/mechel.jpg) no-repeat top left;
}

	.block-mechel div {
		padding: 30px;
		border: 1px solid #ccc;
		border-radius: 4px;
		background-color: #efefef;
	}
	.rgba .block-mechel div {
		background-color: rgba(248, 248, 248, .8);
	}

.block-ipad {
	padding-top: 30px;
	height: 570px;
	background: #fafafa url(../../images/promo/investor/iwatch.jpg) no-repeat top right;
 
}

	.block-ipad h2 {
		padding-right: 530px;
		padding-left: 60px;
	}

	.block-ipad div {
		padding: 10px 550px 0 60px;
		height: 550px;
	}

ol.become-steps {
	counter-reset: li;
	margin-bottom: 20px;
	padding-left: 30px;
	list-style: none;
	font-size: 18px;
}

ol.become-steps li {
	position: relative;
	margin-bottom: 10px;
	min-height: 60px;
	line-height: 25px;
}

ol.become-steps li:before {
	content: counter(li);
	counter-increment: li;
	position: absolute;
	top: 0;
	left: -30px;
	font-size: 40px;
	line-height: 40px;
	color: #7fac31;
}

.form {
	margin: 0 auto;
	width: 700px;
}