@import url(//fonts.googleapis.com/css?family=PT+Sans:400,700&subset=latin,cyrillic);

html,
body {
	font-family: "PT Sans", "Helvetica Neue", Arial;
	font-size: 18px;
	line-height: 23px;
	background-color: #555;
}

a {
	color: #441a8c;
}

.h1, .h2, .h3, .h4, .h5, .h6 {
	font-weight: bold;
	text-rendering: optimizelegibility;
}
.h1, .h2, .h3 {
	line-height: 40px;
}
.h4, .h5, .h6 {
	line-height: 20px;
}
.h1{
	font-size:38.5px;
}
.h2{
	font-size:31.5px;
}
.h3{
	font-size:24.5px;
}
.h4{
	font-size:17.5px;
}
.h5{
	font-size:14px;
}
.h6{
	font-size:11.9px;
}
h2.border {
	margin-top: 30px;
	padding-top: 2px;
	padding-bottom: 5px;
	color: #fff;
	font-weight: normal;
	text-align: center;
}

.flink {
	display: block;
	padding-left: 45px;
	min-height: 40px;
	background-position: left top;
	background-repeat: no-repeat;
}

.flink.one-line {
	padding-top: 10px;
}

.flink.pdf {
	background-image: url(https://www.zerich.com/themes/zericheu/images/pdf.svg);
}

.btn-success {
	background-image: none;
	background-color: #7fac31;
}

.no-margin {
	margin: 0;
}

.container {
	background-color: #fff;
	box-shadow: 0 0 10px #000;
}

.inner {
	padding-left: 60px;
	padding-right: 60px;
}


.headline {

}

	.headline span {
		display: block;
	}
	.headline .line1 {
		font-size: 19px;
		font-weight: normal;
		line-height: 20px;
	}
	.headline .line2 {
		font-size: 85px;
		line-height: 40px;
	}
	.headline .line3 {
		font-size: 81px;
		line-height: 90px;
	}
	.headline .line4 {
		font-size: 73px;
		line-height: 28px;
	}
	.headline .line5 {
		font-size: 121px;
		line-height: 145px;
		color: #7fac31;
	}

	.headline p {
		font-size: 18px;
	}

.description {
	margin-top: 30px;
	margin-bottom: 30px;
	padding: 30px 60px;
	background-color: #e9e9e9;
}

p.button {
	margin-top: 30px;
	margin-bottom: 30px;
}

.fr,
.sr,
.vr {
