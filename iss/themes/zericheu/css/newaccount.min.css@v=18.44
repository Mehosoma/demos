@import url(//fonts.googleapis.com/css?family=PT+Sans:400,700&subset=latin,cyrillic);

h1, h2, h3 {
    font-family: 'PT Sans', sans-serif;
    font-weight: 400;
}

h1 {
    font-size: 1.8em;
}

h2 {
    font-size: 1.6em;
}

h3 {
    font-size: 1.4em;
}

body {
    background-color: #ececec !important;
}

a {
    color: #2e3192;
}

.btn {
    color: #eee;
    background-color: #320886;
    background-image: -moz-linear-gradient(-30deg, #320886 20%, #7353b6 64%);
    background-image: -webkit-gradient(linear, left top, right bottom, color-stop(20%, #320886), color-stop(64%, #7353b6));
    background-image: -webkit-linear-gradient(-30deg, #320886 20%, #7353b6 64%);
    background-image: -o-linear-gradient(-30deg, #320886 20%, #7353b6 64%);
    background-image: -ms-linear-gradient(-30deg, #320886 20%, #7353b6 64%);
    background-image: linear-gradient(25deg, #320886 20%, #7353b6 64%);
    text-shadow: none
}

.btn:active, .btn:hover {
    color: #fff;
    background-color: #320886;
    background-position: 0 0;
}

.second-checkblock {
    font-size: 14px;
    /*border: 1px solid rgba(0, 0, 0, 0.23);*/
}

.js .visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.smallrow {
    margin-left: -10px;
    margin-bottom: 25px;
}

.leftcolumn, .rightcolumn {
    float: left;
    /*margin-left: 20px;*/
}

.btn-checkbox {
    padding-bottom: 15px;
}

.leftcolumn {
    /*width: 575px*/
}

.rightcolumn {
    /*width: 285px;*/
    position: relative;
}

.no-js .js-only {
    display: none;
}

.checkboxbg, .hotline span {
    display: block;
}

.control-group.pull-left {
    /*margin-left: 10px;*/
    margin-bottom: 0;
}

.form-actions {
    margin: 0 0 10px;
    padding: 10px 0 0;
    background-color: transparent
}

.help-block {
    font-size: 13px;
    line-height: 10px
}

.container {
    padding-top: 20px
}

header {
    margin-bottom: 20px;
    padding: 10px 20px;
    background-color: #fff;
}

.boxshadow header {
    box-shadow: 3px 3px 5px #aaa;
}

.no-boxshadow header {
    border-right: 1px solid #999;
    border-bottom: 1px solid #999;
}

.logo {
    margin: 0;
    width: 250px;
    height: 100px;
    text-indent: -9999px;
    overflow: hidden;
    background: url(../images/logo.png) left center no-repeat;
}

.logo-sm {
    margin-left: -10px;
}

.hotline__phone_small {
    line-height: 25px;
    font-size: 20px;
    font-weight: 700;
    margin: 10px 5px 0 0;
}

.hotline {
    margin: 25px 0 20px 60px;
    width: 270px
}

.hotline__label {
    line-height: 14px;
    font-size: 14px;
    margin-bottom: 5px;
}

.hotline__phone {
    line-height: 25px;
    font-size: 25px;
    font-weight: 700;
}

.hotline__tip {
    line-height: 11px;
    font-size: 11px
}

.errmsg, .promo {
    font-size: 12px
}

.callrequest {
    margin-top: 30px
}

.container__inner {
    padding: 0 0 30px
}

.main-success {
    height: 50px;
    padding-left: 60px;
    background: url(data:image/png;base64,) left center no-repeat
}

.container__inner fieldset input[type=text], .container__inner fieldset select {
    /*width: 561px*/
}

#AccountForm .btn-group .btn {
    padding: 0;
    color: #333;
    font-weight: 400;
    background-color: #f5f5f5;
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), to(#e6e6e6));
    background-image: -webkit-linear-gradient(top, #fff, #e6e6e6);
    background-image: -o-linear-gradient(top, #fff, #e6e6e6);
    background-image: linear-gradient(to bottom, #fff, #e6e6e6);
    background-image: -moz-linear-gradient(top, #fff, #e6e6e6);
    background-repeat: repeat-x;
    filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ffffffff', endColorstr='#ffe6e6e6', GradientType=0);
    filter: progid:dximagetransform.microsoft.gradient(enabled=false)
}

#AccountForm .btn-group .btn.active {
    background-image: url(data:image/png;base64,);
    background-position: 10px 11px;
    background-repeat: no-repeat;
    background-size: 15px 15px
}

.checkboxbg {
    padding: 7px 15px 10px 30px;
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA8AAAAPCAYAAAA71pVKAAAABmJLR0QA/wD/AP+gvaeTAAAACXBIWXMAAAsTAAALEwEAmpwYAAAAB3RJTUUH3QsODiMfzES07QAAAChJREFUKM9jZGBg+M9AJmBioACwILEZSdD3n2KbRzWPaiYIGAcsYwAAIXgDHW6WBeYAAAAASUVORK5CYII=);
    background-position: 10px 10px;
    background-repeat: no-repeat;
    border: 1px solid rgba(0, 0, 0, .15);
    border-radius: 5px;
}

.no-js .btn-group input[type=checkbox] {
    margin-top: 2px;
    margin-left: -3px
}

.no-boxshadow .rightcolumn {
    padding-bottom: 15px
}

.promo ul {
    margin-left: 0;
    margin-bottom: 0;
    list-style: none
}

.promo li {
    margin-bottom: 20px;
    padding-left: 70px
}

.promo li.one {
    background: url(data:image/png;base64,) left center no-repeat
}

.promo li.two {
    background: url(data:image/png;base64,) 5px center no-repeat
}

.promo li.three {
    background: url(data:image/png;base64,) left center no-repeat
}

.promo li.four {
    background: url(data:image/png;base64,) left center no-repeat
}
.promo li.five {
    background: url(data:image/png;base64,) left center no-repeat
}

footer {
    font-size: 15px;
    margin-top: 15px;
    padding-top: 10px;
    border-top: 1px solid #000;
}

footer div {
    padding: 0 0 10px;
    font-size: .92em;
    text-shadow: 1px 1px 1px #efefef;
    text-align: center;
}

@media (max-width: 1000px) {
    .footer-media {
        font-size: 15px;
    }

    .promo {
        font-size: 15px;
    }

    .alert-info {
        margin: 80px 0 0 0;
    }

    .btn-return-home {
        /*margin-top: 10px;*/
    }

    .btn-leftcolumn {
        margin-bottom: 10px;
    }
}

@media (max-width: 320px) {
    .btn-return-home {
        margin-top: 10px;
    }
}

@media (max-width: 767px) {
    .hidden-xs, tr.hidden-xs, th.hidden-xs, td.hidden-xs {
        display: none !important
    }
}

@media (min-width: 768px) and (max-width: 991px) {
    .hidden-sm, tr.hidden-sm, th.hidden-sm, td.hidden-sm {
        display: none !important
    }
}

@media (min-width: 992px) and (max-width: 1199px) {
    .hidden-md, tr.hidden-md, th.hidden-md, td.hidden-md {
        display: none !important
    }
}

@media (min-width: 1200px) {
    .hidden-lg, tr.hidden-lg, th.hidden-lg, td.hidden-lg {
        display: none !important
    }
}